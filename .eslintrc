{
  "extends": [
    "plugin:@wordpress/eslint-plugin/recommended"
  ],
  "rules": {
    "linebreak-style": 0,
    "semi": ["error", "never"],
    "quotes": ["error", "single"],
    "object-curly-spacing": ["error", "never"],
    "space-in-parens": ["error", "never"],
    "space-unary-ops": "error"
  }
}